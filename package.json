{
  "name": "versionn",
  "version": "1.0.3",
  "description": "increase version number across package.json, component.json, bower.json and other files",
  "main": "lib/index.js",
  "bin": {
    "versionn": "bin/versionn.js"
  },
  "man": "man/versionn.1",
  "preferGlobal": true,
  "maintainers": [
    {
      "name": "commenthol",
      "email": "commenthol@gmail.com"
    }
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepare": "npm run lint && npm test && npm version",
    "test": "mocha",
    "lint": "eslint '**/*.js'",
    "cover": "istanbul cover _mocha --report lcov --report text -- -R dot --check-leaks"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/commenthol/versionn.git"
  },
  "keywords": [
    "version",
    "component.json",
    "bower.json",
    "package.json"
  ],
  "author": "commenthol",
  "license": "MIT",
  "dependencies": {
    "asyncc": "^2.0.1",
    "semver": "^5.5.0"
  },
  "devDependencies": {
    "mocha": "^5.0.1",
    "shelljs": "^0.8.1"
  },
  "eslintConfig": {
    "extends": "standard",
    "plugins": [
      "standard"
    ]
  }
}
